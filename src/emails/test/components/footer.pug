<!-- START FOOTER MODULE -->
table(width="100%"  align="center")&attributes(tableAtt)
  tr
    td(bgcolor="#e1e1e1" style="padding:12px 0")
      table(width=max_w border="0" cellpadding="10" cellspacing="0" align="center" class="deviceWidth" style="padding:0 10px 0 10px;margin:0 auto;font-weight:normal;font-family: Arial, Helvetica, sans-serif;")
        tr
          td
            table(width="85%" align="left" class="deviceWidth")&attributes(tableAtt)
              each val in footer.paragraphs
                tr
                  td(valign="top" style="padding-bottom:4px;font-weight:normal;font-family: Arial, Helvetica, sans-serif;" class="footer" align="left").
                    !{val}
              tr
                td(valign="top" class="footer" align="left").
                   Lorem ipsum.

        tr
          td(style="padding-bottom:0px;")
            table
              tr
                td(valign="middle" style="vertical-align: middle;" class="logo-container")
                  a(href=footer.logo.url)
                    img(class="full-width" src=img_path+footer.logo.img width="90" alt="Celgene" border="0" style="max-width: 100%;")
                  br
                td(valign="middle" style="vertical-align: middle;" width="14px") &nbsp;
                td(valign="middle" style="vertical-align: middle;")
                  table&attributes(tableAtt)
                    tr
                      td
                        table(class="foot_2 hide" width="85%")&attributes(tableAtt)
                          tr
                            td(class="footer" style="text-align:left;line-height: 1;padding-bottom:5px; font-size: 10px !important;").
                              !{footer.rights1}
                          tr
                            td
                              table(width="100%" class="hide ")&attributes(tableAtt)
                                tr
                                  td
                                    table(width="100%" class="hide ")
                                      tr
                                        each val in footer.rights2
                                          | !{val}

                        <!--[if !mso]><!-->
                        table(style="display:none;" class="show ")&attributes(tableAtt)
                          tr
                            td
                              table(width="100%" class="hide ")
                                tr
                                  each val in footer.rights2
                                    | !{val}
                        <!--<![endif]-->

<!-- END FOOTER MODULE -->
